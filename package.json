{
  "name": "Cordova-react-redux-boilerplate",
  "version": "0.0.0",
  "author": "Farzad YZ",
  "description": "A boilerplate including react, redux, react-router, webpack, cordova",
  "scripts": {
    "server": "export NODE_ENV=development && webpack-dev-server --config webpack.config.js --inline --port 3200",
    "build:webpack": "export NODE_ENV=production && webpack --config webpack.config.js -p",
    "build:cordova": "cordova build android",
    "run:cordova": "cordova run android",
    "build:cordova:release": "cordova build android --release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/farskid/cordova-react-redux-boilerplate"
  },
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.7",
    "redux": "^3.2.1",
    "redux-api-middleware": "^1.0.2",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.3.14",
    "css-loader": "^0.26.0",
    "node-sass": "^3.13.0",
    "postcss-loader": "^1.1.1",
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.6.5",
    "multiparty": "^4.1.2",
    "postcss": "^5.2.6",
    "raw-loader": "^0.5.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.16.2"
  }
}
